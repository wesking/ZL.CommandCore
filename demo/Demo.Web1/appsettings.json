{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "MassTransit": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "log\\{Date}.txt",
          "outputTemplate": "{Timestamp:MM/dd/yy HH:mm:ss.fff} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
  }
